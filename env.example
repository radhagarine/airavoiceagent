# Daily credentials
DAILY_API_KEY=your_daily_api_key
DAILY_API_URL=https://api.daily.co/v1

# Twilio credentials
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token

# Service keys
OPENAI_API_KEY=your_openai_api_key
CARTESIA_API_KEY=your_cartesia_api_key

# Supabase credentials
SUPABASE_URL=your_supabase_url
SUPABASE_KEY=your_supabase_service_key

# LanceDB path (directory where vector databases are stored)
# Set this to the path of your existing LanceDB data directory
LANCEDB_PATH=/path/to/your/lancedb_data

# Monitoring and Observability (Optional - disabled by default)
METRICS_ENABLED=false
TRACING_ENABLED=false
STRUCTURED_LOGGING_ENABLED=false

# Metrics configuration
PUSHGATEWAY_URL=
METRICS_UPDATE_INTERVAL=10
LOG_LEVEL=INFO
LOG_FORMAT=console

# Jaeger configuration (for tracing)
JAEGER_HOST=localhost
JAEGER_PORT=6831

# Optional: Set environment
ENVIRONMENT=development
APP_VERSION=1.0.0

# Cache Configuration
# L1 Cache (In-Memory)
CACHE_L1_SIZE=500
CACHE_L1_TTL=300

# Cache TTL Configuration (in seconds)
CACHE_BUSINESS_TTL=1800      # 30 minutes (business info is stable)
CACHE_KNOWLEDGE_TTL=3600     # 1 hour (knowledge base is stable)  
CACHE_L2_TTL=3600           # Default L2 TTL

# Redis Configuration (for L2 cache)
# Note: Redis cluster nodes are configured in docker-compose.yml
# For external Redis, set these:
# REDIS_HOST=localhost
# REDIS_PORT=6379
# REDIS_PASSWORD=your_redis_password

# Cache Performance Settings
CACHE_COMPRESSION=true       # Enable compression for large values
CACHE_MAX_RETRIES=2         # Number of retries for failed operations
CACHE_RETRY_DELAY=0.1       # Initial retry delay in seconds

# Circuit Breaker Settings
CACHE_CB_THRESHOLD=5        # Failures before circuit breaker opens
CACHE_CB_TIMEOUT=30         # Seconds before trying again

# Cache Warming
CACHE_WARMING=true          # Enable cache warming